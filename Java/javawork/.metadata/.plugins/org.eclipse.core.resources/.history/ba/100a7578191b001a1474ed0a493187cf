package workbook.section3;

public class Test6 {

	public static void main(String[] args) {
		
		Student s1 = new Student();
		Student s2 = new Student();
		Student s3 = new Student();
		
		s1.setName("Kim");
		s1.setKorea(100);
		s1.setEnglish(90);
		s1.setMath(95);
		s1.setScience(89);

		s2.setName("Lee");
		s2.setKorea(60);
		s2.setEnglish(70);
		s2.setMath(99);
		s2.setScience(98);

		s3.setName("Park");
		s3.setKorea(68);
		s3.setEnglish(86);
		s3.setMath(60);
		s3.setScience(40);
		
		
		System.out.println("Kim 錢:" + s1.getAvg()+"\t햬:"+s1.getGrade()+"햬");
		System.out.println("Lee 錢:" + s2.getAvg()+"\t햬:"+s2.getGrade()+"햬");
		System.out.println("Park 錢:" + s3.getAvg()+"\t햬:"+s3.getGrade()+"햬");
		
		
		Student s4 = new Student("Kim", 400, 90, 95, 89);
		Student s5 = new Student("Lee", 60, 70, 99, 98);
		Student s6 = new Student("Park", 68, 86, 60, 40);
		
		System.out.println("Kim 錢:" + s4.getAvg()+"\t햬:"+s4.getGrade()+"햬");
		System.out.println("Lee 錢:" + s5.getAvg()+"\t햬:"+s5.getGrade()+"햬");
		System.out.println("Park 錢:" + s6.getAvg()+"\t햬:"+s6.getGrade()+"햬");
		
		

	}

}
