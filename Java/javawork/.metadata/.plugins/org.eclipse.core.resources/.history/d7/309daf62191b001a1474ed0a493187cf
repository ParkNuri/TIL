package workbook.section3;

public class Test6 {

	public static void main(String[] args) {
		Student s1 = new Student("Kim", 100, 90, 95, 89);
		Student s2 = new Student("Lee", 60, 70, 99, 98);
		Student s3 = new Student("Park", 68, 86, 60, 40);
		
		System.out.println("Kim 錢:" + s1.getAvg()+"\t햬:"+s1.getGrade()+"햬");
		System.out.println("Lee 錢:" + s2.getAvg()+"\t햬:"+s2.getGrade()+"햬");
		System.out.println("Park 錢:" + s3.getAvg()+"\t햬:"+s3.getGrade()+"햬");
		
		

	}

}
