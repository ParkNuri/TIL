package workbook.section4;

public class AccountTest2 {

	public static void main(String[] args) {
		Account[] a = new Account[5];
		
		for(int i = 1;i < 6;i++) {
			a[i] = new Account("221-0101-211"+i, 100000, 4.5);
		}
		
		for(int i = 1;i < 6;i++) {
			a[i].print();
		}

		for(int i = 1;i < 6;i++) {
			a[i].setInterestRate(3.7);
		}

		for(int i = 1;i < 6;i++) {
			a[i].getInterestRate();
		}
		

	}

}
