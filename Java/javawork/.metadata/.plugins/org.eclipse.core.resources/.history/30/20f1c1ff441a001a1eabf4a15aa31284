package chap05;

public class Array2DTest01 {

	public static void main(String[] args) {
		//2차원배열의 선언과 생성
		int[][] myarr = new int[2][3];
		//2차원배열의 초기화
		myarr[0][0] = 100;
		myarr[1][1] = 200;
		//myarr[2][2] = 300; -> Exception발생
		
		System.out.println("myarr의 2번째 요소가 참고하는 배열의 0번 요소의 값"+myarr[1][0]);
		
		
	}

}
